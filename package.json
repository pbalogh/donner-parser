{
    "name": "donner-parser",
    "version": "1.0.0",
    "description": "A left-right parser that uses a trie-based DFA for speed and simplicity",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "test": "jest --config jest.config.js",
        "build": "tsc",
        "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
        "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
        "prepare": "npm run build",
        "prepublishOnly": "npm test && npm run lint",
        "preversion": "npm run lint",
        "version": "npm run format && git add -A src",
        "postversion": "git push && git push --tags"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/pbalogh/donner-parser.git"
    },
    "keywords": [],
    "author": "Peter Balogh",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/pbalogh/donner-parser/issues"
    },
    "homepage": "https://github.com/pbalogh/donner-parser#readme",
    "devDependencies": {
        "@types/jest": "^25.2.1",
        "@typescript-eslint/eslint-plugin": "2.28.0",
        "@typescript-eslint/parser": "2.28.0",
        "babel-eslint": "10.1.0",
        "babel-jest": "^25.3.0",
        "eslint": "6.8.0",
        "eslint-config-airbnb": "18.1.0",
        "eslint-config-airbnb-typescript": "7.2.1",
        "eslint-config-prettier": "6.10.1",
        "eslint-config-react-app": "5.2.1",
        "eslint-import-resolver-typescript": "2.0.0",
        "eslint-loader": "4.0.0",
        "eslint-plugin-flowtype": "4.7.0",
        "eslint-plugin-import": "2.20.2",
        "eslint-plugin-jsx-a11y": "6.2.3",
        "eslint-plugin-react": "7.19.0",
        "eslint-plugin-react-hooks": "3.0.0",
        "jest": "^25.3.0",
        "prettier": "^2.0.4",
        "ts-jest": "^25.3.1",
        "typescript": "^3.8.3"
    },
    "dependencies": {
        "eslint-plugin-prettier": "^3.1.3"
    },
    "files": [
        "lib/**/*"
    ]
}
